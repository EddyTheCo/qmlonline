import QtQuick.Controls
import QtQuick 2.0
import QtQrDec
Item {
    anchors.fill:parent
    QrTextArea
    {
        id: butt
        width: 350
        height:500
        visible:true
        closePolicy: Popup.NoAutoClose
        anchors.centerIn: parent
    }

}

