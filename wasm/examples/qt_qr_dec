/*
*  Shows the use of a QRCODE decoder.
*  The library is found on https://github.com/EddyTheCo/qrCode
*
*/

import QtQuick.Controls
import QtQuick 2.0
import QtQrDec
Item {
    anchors.fill:parent
    QrTextCamPop
    {
        closePolicy: Popup.NoAutoClose
        width: 300
        height:500
        anchors.centerIn: Overlay.overlay
        visible:true
    }

}

